<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="587.0" prefWidth="786.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <VBox prefHeight="587.0" prefWidth="786.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Pane fx:id="paneView" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
            <TextField fx:id="dataToAdd" alignment="CENTER" text="Test" />
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button fx:id="showTenDaysButton" mnemonicParsing="false" onAction="#updateTimeShow" text="10 Days" />
                  <Button fx:id="show30DaysButton" mnemonicParsing="false" onAction="#updateTimeShow" text="30 Days" />
                  <Button fx:id="showAllButton" mnemonicParsing="false" onAction="#updateTimeShow" text="All" />
               </children>
            </HBox>
            <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#updateData" text="Update">
               <VBox.margin>
                  <Insets left="365.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
